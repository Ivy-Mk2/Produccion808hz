@startuml
skinparam style strictuml

package "Models" {
  class Cliente {
    id
    dni
    nombre
    contacto
  }
  class Producto {
    id
    nombre
    precio
    estado
  }
  class Cotizacion {
    id
    clienteId
    fecha
    items
  }
  class CotizacionItem {
    productoId
    cantidad
    precioUnitario
  }
  class Reserva {
    fecha
    hora
    recurso
    estado
  }
  class Tecnico {
    id
    nombre
    especialidad
  }
  class Accion {
    tipo
    payload
  }
}

Cotizacion --> "*" CotizacionItem
Cliente --> "*" Cotizacion
Reserva --> Producto
Tecnico --> Reserva

package "Services" {
  class QuoteService
  class ReservationService
  class InventoryService
  class UndoRedoService
  class LogisticsService
  class PackageService
  class ClientIndexService
  class ProductIndexService
  class JsonStorageService
}

package "DataStructures" {
  class SinglyLinkedList
  class DoublyLinkedList
  class CircularLinkedList
  class Stack
  class Queue
  class BinarySearchTree
  class Graph
  class HashTable
}

QuoteService .. SinglyLinkedList
ReservationService .. DoublyLinkedList
InventoryService .. CircularLinkedList
UndoRedoService .. Stack
LogisticsService .. Queue
ClientIndexService .. BinarySearchTree
ProductIndexService .. BinarySearchTree
LogisticsService .. Graph
PackageService .. Graph
UserService .. HashTable

JsonStorageService --> Models
@enduml